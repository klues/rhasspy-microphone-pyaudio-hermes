#!/usr/bin/env bash
if [[ -z "${rhasspymicrophone_pyaudio_hermes_dir}" ]]; then
    this_dir="$( cd "$( dirname "$0" )" && pwd )"
    rhasspymicrophone_pyaudio_hermes_dir="${this_dir}/../lib/rhasspymicrophone_pyaudio_hermes"
fi

"${rhasspymicrophone_pyaudio_hermes_dir}/rhasspymicrophone_pyaudio_hermes" "$@"
